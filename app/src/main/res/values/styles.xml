<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryUS</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkUS</item>
        <item name="colorAccent">@color/colorAccentUS</item>
    </style>

    <style name="AppThemeJP" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimaryJP</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDarkJP</item>
        <item name="colorAccent">@color/colorAccentJP</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>


    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

</resources>
